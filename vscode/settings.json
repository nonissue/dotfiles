{
  
  "autoimport.filesToScan": "**/*.{ts,tsx}",
  "autoimport.showNotifications": true,
  "city-lights-icons-vsc.hidesExplorerArrows": true,
  "css.lint.unknownAtRules": "ignore",
  "css.lint.validProperties": ["composes", "apply"],
  "css.validate": true,
  "editor.accessibilitySupport": "off",
  "editor.cursorStyle": "underline",
  "editor.fontFamily": "'Input', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13.5,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 90,
  "eslint.packageManager": "yarn",
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "faunadb.adminSecretKey": "fnADvAU-q-ACAHFwmh5bTceeQvqBg92VsgSH2dS3",
  "git.enableSmartCommit": true,
  "git.showPushSuccessNotification": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "lua.format.indentCount": 4,
  "lua.preferLuaCheckErrors": true,
  "lua.targetVersion": "5.3",
  "npm-intellisense.packageSubfoldersIntellisense": true,
  "npm.packageManager": "yarn",
  "prettier.jsxBracketSameLine": true,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.useEditorConfig": false,
  "remote.SSH.connectTimeout": 30,
  "remote.SSH.remoteServerListenOnSocket": true,
  "remote.SSH.useLocalServer": false,
  "search.showLineNumbers": true,
  "security.workspace.trust.untrustedFiles": "open",
  "stylelint.packageManager": "yarn",
  "tailwindCSS.emmetCompletions": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.scrollback": 500,
  // "terminal.integrated.shell.linux": "/usr/bin/fish",
  "terminal.integrated.windowsEnableConpty": false,
  "todo.formatting.enabled": true,
  "tslint.packageManager": "yarn",
  "typescript.autoClosingTags": false,
  "typescript.preferences.quoteStyle": "single",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.newWindowDimensions": "maximized",
  "window.openFoldersInNewWindow": "on",
  "workbench.colorTheme": "Moonlight II",
  "workbench.editor.highlightModifiedTabs": true,
  // "workbench.editor.tabSizing": "shrink",
  "workbench.fontAliasing": "auto",
  "workbench.iconTheme": "quill-icons",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.sideBar.location": "left",
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 8,
  "workbench.tree.renderIndentGuides": "always",
  "yarn.enableTouchbar": true,
  "[css]": {
    "editor.suggest.insertMode": "replace"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "breadcrumbs.showClasses": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "dash.languageIdToDocsetMap": {
    "lua": ["lua", "hammerspoon"],
    "typescriptreact": [
      "typescript",
      "react",
      "javascript",
      "tailwindcss",
      "next.js"
    ]
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "remote.SSH.remotePlatform": {
    "hetz": "linux"
  },
  "tailwindCSS.includeLanguages": {
    "typescriptreact": "html",
    "plaintext": "javascript"
  },
  "todo.file.exclude": [
    "**/.!(todo|todos|task|tasks)/**",
    "**/_output/**",
    "**/bower_components/**",
    "**/build/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/out/**",
    "**/output/**",
    "**/release/**",
    "**/releases/**",
    "**/static/**",
    "**/target/**",
    "**/third_party/**",
    "**/vendor/**",
    "**/lambda/**"
  ]
}
