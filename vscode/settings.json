{
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.guides.highlightActiveIndentation": "always",
  "git.autofetch": true,
  "Lua.codeLens.enable": true,
  "Lua.completion.displayContext": 1,
  "redhat.telemetry.enabled": false,
  "workbench.fontAliasing": "auto",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always",
  "editor.formatOnSave": true,
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "editor.formatOnSaveMode": "modifications",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Matey",
  "prisma.showPrismaDataPlatformNotification": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "telemetry.telemetryLevel": "crash",
  "extensions.autoUpdate": false,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "editor.defaultFormatter": "yzhang.markdown-all-in-one",
  "workbench.editor.tabSizingFixedMinWidth": 70,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.preferredLightColorTheme": "Feather",
  "symbols.hidesExplorerArrows": false,
  "workbench.iconTheme": "symbols",
  "workbench.preferredDarkColorTheme": "Aylin",
  "workbench.editorAssociations": {
    "*.plist": "default"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
